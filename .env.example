# PostgreSQL
POSTGRES_DB=qqqq_agents
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Application
DEBUG=false

# Interactive Brokers Settings
# Port reference:
#   7497 = TWS Paper Trading
#   7496 = TWS Live Trading
#   4001 = IB Gateway Paper Trading
#   4002 = IB Gateway Live Trading
IB_HOST=host.docker.internal
IB_PORT=7497
IB_CLIENT_ID=1
IB_READONLY=false
BROKER_ACCOUNT_ID=your_ib_account_id_here

# Trading Strategy Settings
MAX_POSITION_PCT=0.25
MAX_DRAWDOWN_PCT=0.15
VIX_SHUTDOWN_THRESHOLD=45.0
SPREAD_WIDTH=25
TARGET_CREDIT_MIN=0.55
TARGET_CREDIT_MAX=0.70
MAX_DELTA=0.12

# Orchestrator Schedule (ET timezone)
EXECUTION_HOUR=15
EXECUTION_MINUTE=45

# Market Data API (optional - IB provides market data)
MARKET_DATA_API_KEY=your_market_data_api_key_here

# Robinhood Crypto Trading API
# Generate API keys at https://robinhood.com/account/crypto
# ROBINHOOD_PRIVATE_KEY should be base64-encoded ED25519 private key
ROBINHOOD_API_KEY=your_robinhood_api_key_here
ROBINHOOD_PRIVATE_KEY=your_base64_encoded_private_key_here
